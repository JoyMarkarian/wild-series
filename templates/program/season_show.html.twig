{% extends 'base.html.twig' %}

{% block body %}
    <h1>{{ program.title }} - Season {{ season.number }}</h1>
    <p>{{ season.description }}</p>
    
    <h2>Episodes</h2>
    <ul>
        {% for episode in episodes %}
            <li>{{ episode.title }} - {{ episode.number }}</li>
        {% endfor %}
    </ul>
{% endblock %}